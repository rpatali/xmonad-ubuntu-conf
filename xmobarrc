Config { 
   font     = "xft:Ubuntu Mono-13",
   bgColor  = "black",
   fgColor  = "grey",
   position = TopW L 95, 
   commands = [ 
      Run Cpu [
         "-H","50",
         "--high","red"
      ] 10,
      Run Memory [
         "-t","Mem: <usedratio>%"
      ] 10,
      Run Date "%a %b %_d %l:%M" "date" 10,
      Run StdinReader
   ],
   sepChar  = "%",
   alignSep = "}{",
   template = "%StdinReader% }{ %cpu% | %memory% |  <fc=#e6744c>%date%</fc>  "
}
